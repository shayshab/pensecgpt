    "zod": "^3.22.4",
    "bullmq": "^5.3.0",
    "ioredis": "^5.3.2",
    "axios": "^1.6.2",
    "openai": "^4.20.1",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
